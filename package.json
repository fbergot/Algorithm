    "homepage": "https://github.com/fbergot/Algorithm#readme",
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.2",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "eslint": "^8.50.0",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "ts-jest": "^29.1.1",
